<Configuration status = "INFO">
    <Appenders>
        <Console name ="Cobsole" target = "SYSTEM_OUT">
           <PatternLayout pattern ="%d{HH:mm:ss.SSS} [%t} %logger {36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level = "info">
            <AppenderRef  ref = "Console"/>
        </Root>
    </Loggers>
</Configuration>